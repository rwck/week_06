  @user_name.to_s end -->

  <tbody>
    <% @projects.each do |project| %>

    <tr>
      <td><%= project.name %></td>
      <td><%= project.user.id %></td>
      <!-- <td><%= @user_name %></td> -->
      <td>
        <%= @users.find(project.user_id).firstname %>
        <%= @users.find(project.user_id).lastname %>
